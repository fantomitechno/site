{
    "extends": "astro/tsconfigs/strict",
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "strictNullChecks": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}